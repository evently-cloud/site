{
    "name": "evently",
    "description": "Evently web site",
    "license": "CC-BY-NC-ND-4.0",
    "private": false,
    "main": "index.js",
    "scripts": {
        "start": "ELEVENTY_PRODUCTION=false eleventy --serve & postcss src/css/main.css --o _tmp/main.css --watch",
        "build": "ELEVENTY_PRODUCTION=true eleventy & postcss src/css/main.css --o _site/css/main.css",
        "clean": "rm -rf _site & rm -rf _tmp"
    },
    "dependencies": {
        "@11ty/eleventy": "0.12.1",
        "@11ty/eleventy-navigation": "0.3.2",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "@tailwindcss/forms": "0.3.4",
        "autoprefixer": "10.4.0",
        "eleventy-plugin-embed-everything": "^1.14.1",
        "html-minifier": "4.0.0",
        "markdown-it": "12.3.0",
        "markdown-it-anchor": "8.4.1",
        "markdown-it-codetabs": "1.2.0",
        "markdown-it-container": "3.0.0",
        "markdown-it-image-figures": "^2.1.0",
        "markdown-it-prism": "2.2.1",
        "postcss-cli": "9.0.2",
        "tailwindcss": "2.2.19"
    }
}
